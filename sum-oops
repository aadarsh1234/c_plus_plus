
#include <iostream>
using namespace std;
class add{
    private:
    int a,b,sum1;
    public:
    void input();
    void sum();
    
};
void add::input(){
    cout<<"Enter the element of a and b:";
    cin>>a>>b;
}
void add::sum(){
    sum1=a+b;
    cout<<"addition is:"<<sum1<<endl;
    

}

int main() 
{
add a;
a.input();
a.sum();
    return 0;
}
